{
  "name": "solid-sortable",
  "version": "0.1.0",
  "description": "reactive drag and drop lists",
  "license": "MIT",
  "author": "Wil Gaboury",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilgaboury/solid-sortable.git"
  },
  "homepage": "https://github.com/wilgaboury/solid-sortable#readme",
  "bugs": {
    "url": "https://github.com/wilgaboury/solid-sortable/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "fix": "pnpm run lint:code --fix",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@solidjs/router": "^0.13.3",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.10",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "^23.0.1",
    "prettier": "3.1.1",
    "solid-js": "^1.8.7",
    "tsup": "^8.0.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  },
  "dependencies": {
    "@solid-primitives/trigger": "^1.0.11"
  }
}
